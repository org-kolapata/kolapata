<odoo>
  <data>
      <record id="pos_kolap_config_view_form" model="ir.ui.view">
        <field name="name">pos.iot.config.form.view</field>
        <field name="model">pos.config</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='title']" position="after">
              <div id="restaurant_on_create" class="row mt16 o_settings_container">
                    <setting>
                            <field name="show_closing_fields" />
                    </setting>
                </div>
            </xpath>
        </field>
      </record>
  </data>
</odoo>
