<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <!-- Sequences for Inter Company Transfer-->
    <data>
        <record id="requisition_transfer_seq" model="ir.sequence">
            <field name="name">Requisition Transfer Sequence</field>
            <field name="code">requisition.transfer.sequence</field>
            <field name="prefix">PURCHASE/REQ/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="requisition_recv_seq" model="ir.sequence">
            <field name="name">Requisition Receive Sequence</field>
            <field name="code">requisition.receive.sequence</field>
            <field name="prefix">RECV/REQ/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>